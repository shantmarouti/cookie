{
    "$schema": "http://json.schemastore.org/prettierrc",
    "arrowParens": "always",
    "bracketSpacing": true,
    "overrides": [
        {
            "files": [
                "**/*.json"
            ],
            "options": {
                "printWidth": 200,
                "tabWidth": 2
            }
        }
    ],
    "printWidth": 100,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 4,
    "trailingComma": "none",
    "useTabs": false
}
